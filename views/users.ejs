<!doctype html>
<html>
  <head>
    <title>Welcome to My Forum</title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>Users</h1>
    <!-- the users are listed and each user has a href which takes it to another html which lists all the users relevant to that user -->
    <ul>
      <% users.forEach(function(user) { %>
        <li><a href="/users/<%= user.user_id %>"><%= user.username %>: <%= user.email %></a></li>
      <% }) %>
    </ul>
  </body>
</html>
