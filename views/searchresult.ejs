<!doctype html>
<html>
  <head>
    <title>Books Listed in</title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>Search Results in</h1>
    <!-- the results of the user's search is displayed with the post and the name of the topic displayed next to it -->
    <h2>Your search results matched to the following posts:</h2>
    <ul>
        <% posts.forEach(function(post){%> 
            <li>"<%=post.text %>": <%=post.topicname %></li>
            <% }) %>
    </ul>
  </body>
</html>