<!doctype html>
<html>
  <head>
    <title>Welcome to My Forum</title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1>Posts</h1>
    <!-- this html displays all the posts for the topic that is selected in the topics list -->
    <ul>
        <% posts.forEach(function(post){%> 
        <li>"<%=post.text %>"</li>
        <% }) %>
    </ul>
  </body>
</html>
